{
	"compileOnSave": false,
	"compilerOptions": {
		"baseUrl": "./",
		"outDir": "./dist/out-tsc",
    "sourceMap": true,
    "esModuleInterop": true,
    "declaration": false,
		"experimentalDecorators": true,
		"module": "es2020",
		"moduleResolution": "node",
		"strict": true,
		"paths": {
			"chartiq/*": [
				"node_modules/@chartiq/technical-analysis/*",
				"node_modules/@chartiq/chart2music/*",
				"node_modules/@chartiq/core/*",
				"node_modules/@chartiq/component-ui/*",
				"node_modules/@chartiq/web-components/*",
				"node_modules/@chartiq/active-trader/*",
				"node_modules/@chartiq/crossplot/*",
				"node_modules/@chartiq/gonogo/*",
				"node_modules/@chartiq/institutional/*",
				"node_modules/@chartiq/trading-central/*",
				"node_modules/chartiq/*"
			],
			"keyDir/*": [
				"/path/to/your/keyfile/*",
				"node_modules/chartiq/*"
			]
		},
	"importHelpers": true,
	"target": "es6",
	"typeRoots": ["node_modules/@types", "node_modules/@chartiq", "src/types"],
	"types": [],
	"lib": ["es2020", "dom"],
	"skipLibCheck": true
	},
	"angularCompilerOptions": {
		"fullTemplateTypeCheck": true,
		"strictInjectionParameters": true
	},
	"include": [
		"src/**/*.ts",
		"src/**/*.d.ts"
	]
}
